   "metadata": {},
   "outputs": [],
   "source": [
    "# Since we want to use more than one column, we can't just simply use np.asarray(). Instead, we can use the to_dict() method together with sklearn's DictVectorizer.\n",
    "quartier_features = quartier_selected.to_dict(orient='records')"
   ]
